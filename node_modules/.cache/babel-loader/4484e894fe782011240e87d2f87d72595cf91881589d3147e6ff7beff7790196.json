{"ast":null,"code":"import UploadCarton from './components/UploadCarton.vue';\nimport { db } from './firebase';\nimport { collection, addDoc } from 'firebase/firestore';\nexport default {\n  components: {\n    UploadCarton\n  },\n  methods: {\n    async handleImageUploaded(data) {\n      try {\n        const docRef = await addDoc(collection(db, 'cartones'), {\n          serial: data.serial,\n          // Ahora debería ser \"LM0004\"\n          imageUrl: data.imageUrl,\n          estado: 'disponible',\n          createdAt: new Date(),\n          vendedorId: null\n        });\n        console.log('Cartón guardado con ID:', docRef.id);\n        alert('Cartón guardado exitosamente en Firestore');\n      } catch (error) {\n        console.error('Error al guardar el cartón:', error);\n        alert('Error al guardar en Firestore: ' + error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["UploadCarton","db","collection","addDoc","components","methods","handleImageUploaded","data","docRef","serial","imageUrl","estado","createdAt","Date","vendedorId","console","log","id","alert","error","message"],"sources":["D:\\Lotweb\\venta-cartones\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>Venta de Cartones</h1>\n    <UploadCarton @image-uploaded=\"handleImageUploaded\" />\n  </div>\n</template>\n\n<script>\nimport UploadCarton from './components/UploadCarton.vue';\nimport { db } from './firebase';\nimport { collection, addDoc } from 'firebase/firestore';\n\nexport default {\n  components: {\n    UploadCarton\n  },\n  methods: {\n    async handleImageUploaded(data) {\n      try {\n        const docRef = await addDoc(collection(db, 'cartones'), {\n          serial: data.serial, // Ahora debería ser \"LM0004\"\n          imageUrl: data.imageUrl,\n          estado: 'disponible',\n          createdAt: new Date(),\n          vendedorId: null\n        });\n        console.log('Cartón guardado con ID:', docRef.id);\n        alert('Cartón guardado exitosamente en Firestore');\n      } catch (error) {\n        console.error('Error al guardar el cartón:', error);\n        alert('Error al guardar en Firestore: ' + error.message);\n      }\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  text-align: center;\n  margin: 20px;\n}\n</style>"],"mappings":"AAQA,OAAOA,YAAW,MAAO,+BAA+B;AACxD,SAASC,EAAC,QAAS,YAAY;AAC/B,SAASC,UAAU,EAAEC,MAAK,QAAS,oBAAoB;AAEvD,eAAe;EACbC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,OAAO,EAAE;IACP,MAAMC,mBAAmBA,CAACC,IAAI,EAAE;MAC9B,IAAI;QACF,MAAMC,MAAK,GAAI,MAAML,MAAM,CAACD,UAAU,CAACD,EAAE,EAAE,UAAU,CAAC,EAAE;UACtDQ,MAAM,EAAEF,IAAI,CAACE,MAAM;UAAE;UACrBC,QAAQ,EAAEH,IAAI,CAACG,QAAQ;UACvBC,MAAM,EAAE,YAAY;UACpBC,SAAS,EAAE,IAAIC,IAAI,CAAC,CAAC;UACrBC,UAAU,EAAE;QACd,CAAC,CAAC;QACFC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAER,MAAM,CAACS,EAAE,CAAC;QACjDC,KAAK,CAAC,2CAA2C,CAAC;MACpD,EAAE,OAAOC,KAAK,EAAE;QACdJ,OAAO,CAACI,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;QACnDD,KAAK,CAAC,iCAAgC,GAAIC,KAAK,CAACC,OAAO,CAAC;MAC1D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}