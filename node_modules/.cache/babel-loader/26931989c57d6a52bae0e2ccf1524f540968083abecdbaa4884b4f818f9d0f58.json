{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { auth } from '../firebase';\nimport { signInWithEmailAndPassword, onAuthStateChanged, signOut } from 'firebase/auth';\nimport { createUserWithEmailAndPassword } from 'firebase/auth';\nimport { db } from '../firebase';\nimport { doc, setDoc, collection, query, where, onSnapshot, getDocs, addDoc, updateDoc } from 'firebase/firestore';\nimport { getStorage, ref, uploadBytes, getDownloadURL } from 'firebase/storage';\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: '',\n      loading: false,\n      user: null,\n      showAddSellerModal: false,\n      newSellerEmail: '',\n      newSellerPassword: '',\n      newSellerName: '',\n      selectedSeller: '',\n      cartonFrom: '',\n      cartonTo: '',\n      sellers: [],\n      cartons: [],\n      availableCartons: [],\n      assignedCartons: [],\n      soldCartons: [],\n      files: []\n    };\n  },\n  mounted() {\n    onAuthStateChanged(auth, user => {\n      this.user = user;\n      if (user) {\n        if (user.email === 'master1@example.com') {\n          this.loadSellers();\n          this.loadCartons();\n          this.loadCartonStats();\n        } else if (user.email.startsWith('vendedor')) {\n          this.$router.push('/vendedor');\n        }\n      }\n    });\n  },\n  methods: {\n    async login() {\n      this.loading = true;\n      this.error = '';\n      try {\n        console.log('Intentando iniciar sesión con:', this.email, this.password);\n        await signInWithEmailAndPassword(auth, this.email, this.password);\n        console.log('Sesión iniciada con éxito');\n      } catch (error) {\n        console.error('Error al iniciar sesión:', error.code, error.message);\n        this.error = `Error: ${error.code} - ${error.message}`;\n      } finally {\n        this.loading = false;\n      }\n    },\n    async addSeller() {\n      if (!this.newSellerEmail || !this.newSellerPassword || !this.newSellerName) {\n        alert('Por favor, complete todos los campos.');\n        return;\n      }\n      try {\n        const userCredential = await createUserWithEmailAndPassword(auth, this.newSellerEmail, this.newSellerPassword);\n        const user = userCredential.user;\n        await setDoc(doc(db, 'vendedores', user.uid), {\n          email: this.newSellerEmail,\n          nombre: this.newSellerName,\n          saldoGenerado: 0\n        });\n        alert('Vendedor agregado exitosamente.');\n        this.showAddSellerModal = false;\n        this.newSellerEmail = '';\n        this.newSellerPassword = '';\n        this.newSellerName = '';\n        this.loadSellers();\n      } catch (error) {\n        console.error('Error al agregar vendedor:', error);\n        alert('Error al agregar vendedor: ' + error.message);\n      }\n    },\n    async logout() {\n      try {\n        await signOut(auth);\n        this.user = null;\n        this.$router.push('/login');\n      } catch (error) {\n        console.error('Error al cerrar sesión:', error);\n        alert('Error al cerrar sesión: ' + error.message);\n      }\n    },\n    loadSellers() {\n      const sellersQuery = query(collection(db, 'vendedores'));\n      onSnapshot(sellersQuery, snapshot => {\n        this.sellers = snapshot.docs.map(doc => ({\n          id: doc.id,\n          ...doc.data()\n        }));\n      });\n    },\n    loadCartons() {\n      const cartonsQuery = query(collection(db, 'cartones'));\n      onSnapshot(cartonsQuery, snapshot => {\n        this.cartons = snapshot.docs.map(doc => ({\n          id: doc.id,\n          ...doc.data()\n        }));\n      });\n    },\n    loadCartonStats() {\n      const availableQuery = query(collection(db, 'cartones'), where('vendedorId', '==', null), where('estado', '==', 'disponible'));\n      onSnapshot(availableQuery, snapshot => {\n        this.availableCartons = snapshot.docs.map(doc => ({\n          id: doc.id,\n          serial: doc.data().serial\n        })).sort((a, b) => parseInt(a.serial.replace('LM', '')) - parseInt(b.serial.replace('LM', '')));\n      });\n      const assignedQuery = query(collection(db, 'cartones'), where('estado', '==', 'asignado'));\n      onSnapshot(assignedQuery, snapshot => {\n        this.assignedCartons = snapshot.docs.map(doc => ({\n          id: doc.id,\n          serial: doc.data().serial,\n          vendedorId: doc.data().vendedorId\n        }));\n      });\n      const soldQuery = query(collection(db, 'cartones'), where('estado', '==', 'vendido'));\n      onSnapshot(soldQuery, snapshot => {\n        this.soldCartons = snapshot.docs.map(doc => ({\n          id: doc.id,\n          serial: doc.data().serial\n        }));\n      });\n    },\n    handleFileUpload(event) {\n      this.files = Array.from(event.target.files);\n    },\n    async uploadCartons() {\n      if (this.files.length === 0) {\n        alert('Por favor, selecciona al menos una imagen.');\n        return;\n      }\n      const storage = getStorage();\n      const uploadPromises = this.files.map(async (file, index) => {\n        const serial = `LM${Date.now()}${index}`;\n        const storageRef = ref(storage, `cartones/${serial}`);\n        await uploadBytes(storageRef, file);\n        const url = await getDownloadURL(storageRef);\n        await addDoc(collection(db, 'cartones'), {\n          serial: serial,\n          url: url,\n          estado: 'disponible',\n          vendedorId: null,\n          createdAt: new Date()\n        });\n      });\n      try {\n        await Promise.all(uploadPromises);\n        alert('Cartones subidos exitosamente.');\n        this.files = [];\n      } catch (error) {\n        console.error('Error al subir cartones:', error);\n        alert('Error al subir cartones: ' + error.message);\n      }\n    },\n    async assignCartons() {\n      if (!this.selectedSeller || !this.cartonFrom || !this.cartonTo) {\n        alert('Por favor, selecciona un vendedor y un rango de cartones.');\n        return;\n      }\n      const fromIndex = this.availableCartons.findIndex(carton => carton.id === this.cartonFrom);\n      const toIndex = this.availableCartons.findIndex(carton => carton.id === this.cartonTo);\n      if (fromIndex === -1 || toIndex === -1 || fromIndex > toIndex) {\n        alert('Rango de cartones inválido.');\n        return;\n      }\n      const cartonsToAssign = this.availableCartons.slice(fromIndex, toIndex + 1);\n      const updatePromises = cartonsToAssign.map(async carton => {\n        await updateDoc(doc(db, 'cartones', carton.id), {\n          vendedorId: this.selectedSeller,\n          estado: 'asignado'\n        });\n      });\n      try {\n        await Promise.all(updatePromises);\n        alert('Cartones asignados exitosamente.');\n        this.selectedSeller = '';\n        this.cartonFrom = '';\n        this.cartonTo = '';\n      } catch (error) {\n        console.error('Error al asignar cartones:', error);\n        alert('Error al asignar cartones: ' + error.message);\n      }\n    },\n    refreshCartonList() {\n      this.loadCartons();\n      this.loadCartonStats();\n    },\n    async downloadReport() {\n      const cartons = await getDocs(collection(db, 'cartones'));\n      const reportData = cartons.docs.map(doc => {\n        const data = doc.data();\n        return `${data.serial},${data.estado},${data.vendedorId || 'Sin asignar'}`;\n      }).join('\\n');\n      const blob = new Blob([`Serial,Estado,Vendedor\\n${reportData}`], {\n        type: 'text/csv'\n      });\n      const url = window.URL.createObjectURL(blob);\n      const a = document.createElement('a');\n      a.href = url;\n      a.download = 'reporte_cartones.csv';\n      a.click();\n      window.URL.revokeObjectURL(url);\n    },\n    viewSellerDetails(seller) {\n      alert(`Detalles del vendedor:\\nNombre: ${seller.nombre}\\nEmail: ${seller.email}\\nSaldo Generado: $${seller.saldoGenerado}`);\n    }\n  }\n};","map":{"version":3,"names":["auth","signInWithEmailAndPassword","onAuthStateChanged","signOut","createUserWithEmailAndPassword","db","doc","setDoc","collection","query","where","onSnapshot","getDocs","addDoc","updateDoc","getStorage","ref","uploadBytes","getDownloadURL","name","data","email","password","error","loading","user","showAddSellerModal","newSellerEmail","newSellerPassword","newSellerName","selectedSeller","cartonFrom","cartonTo","sellers","cartons","availableCartons","assignedCartons","soldCartons","files","mounted","loadSellers","loadCartons","loadCartonStats","startsWith","$router","push","methods","login","console","log","code","message","addSeller","alert","userCredential","uid","nombre","saldoGenerado","logout","sellersQuery","snapshot","docs","map","id","cartonsQuery","availableQuery","serial","sort","a","b","parseInt","replace","assignedQuery","vendedorId","soldQuery","handleFileUpload","event","Array","from","target","uploadCartons","length","storage","uploadPromises","file","index","Date","now","storageRef","url","estado","createdAt","Promise","all","assignCartons","fromIndex","findIndex","carton","toIndex","cartonsToAssign","slice","updatePromises","refreshCartonList","downloadReport","reportData","join","blob","Blob","type","window","URL","createObjectURL","document","createElement","href","download","click","revokeObjectURL","viewSellerDetails","seller"],"sources":["D:\\Lotweb\\venta-cartones\\src\\components\\LoginPage.vue"],"sourcesContent":["```vue\r\n<template>\r\n  <div class=\"min-h-screen flex items-center justify-center bg-gradient-to-br from-indigo-900 to-purple-300\">\r\n    <!-- Login Form (visible if not authenticated) -->\r\n    <div v-if=\"!user\" class=\"bg-white p-8 rounded-xl shadow-2xl w-full max-w-md transform transition-all duration-500 opacity-0 animate-fade-in\">\r\n      <div class=\"text-center mb-6\">\r\n        <div class=\"flex justify-center mb-4\">\r\n          <span class=\"text-4xl font-bold text-indigo-600 animate-spin-slow\">VC</span>\r\n        </div>\r\n        <h2 class=\"text-3xl font-bold text-gray-800 mb-2\">Venta de Cartones</h2>\r\n        <p class=\"text-gray-600\">Inicia sesión para gestionar tus cartones</p>\r\n      </div>\r\n      <form @submit.prevent=\"login\" class=\"space-y-6\">\r\n        <div>\r\n          <label for=\"email\" class=\"block text-sm font-medium text-gray-700\">Correo Electrónico</label>\r\n          <input\r\n            v-model=\"email\"\r\n            type=\"email\"\r\n            id=\"email\"\r\n            class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n            placeholder=\"master1@example.com\"\r\n            required\r\n          />\r\n        </div>\r\n        <div>\r\n          <label for=\"password\" class=\"block text-sm font-medium text-gray-700\">Contraseña</label>\r\n          <input\r\n            v-model=\"password\"\r\n            type=\"password\"\r\n            id=\"password\"\r\n            class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n            placeholder=\"Sagu7.98\"\r\n            required\r\n          />\r\n        </div>\r\n        <button\r\n          type=\"submit\"\r\n          :disabled=\"loading\"\r\n          class=\"w-full bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300 flex items-center justify-center\"\r\n        >\r\n          <span v-if=\"loading\" class=\"flex items-center\">\r\n            <svg class=\"animate-spin h-5 w-5 mr-2 text-white\" viewBox=\"0 0 24 24\">\r\n              <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\r\n              <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"></path>\r\n            </svg>\r\n            Cargando...\r\n          </span>\r\n          <span v-else>Iniciar Sesión</span>\r\n        </button>\r\n        <p v-if=\"error\" class=\"text-red-500 text-sm text-center\">{{ error }}</p>\r\n      </form>\r\n      <div class=\"mt-4 text-center\">\r\n        <p class=\"text-gray-600 text-sm\">¿Olvidaste tu contraseña? <a href=\"#\" class=\"text-indigo-600 hover:underline transition duration-300\">Recupérala</a></p>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Admin Dashboard (visible for master1) -->\r\n    <div v-if=\"user && user.email === 'master1@example.com'\" class=\"bg-white p-8 rounded-xl shadow-2xl w-full max-w-4xl transform transition-all duration-500 opacity-0 animate-fade-in\">\r\n      <div class=\"text-center mb-6\">\r\n        <div class=\"flex justify-center mb-4\">\r\n          <span class=\"text-4xl font-bold text-indigo-600 animate-spin-slow\">VC</span>\r\n        </div>\r\n        <h2 class=\"text-3xl font-bold text-gray-800 mb-2\">Dashboard Admin</h2>\r\n        <p class=\"text-gray-600\">Gestiona tus cartones y vendedores</p>\r\n      </div>\r\n\r\n      <div class=\"flex justify-end mb-6\">\r\n        <button\r\n          @click=\"logout\"\r\n          class=\"bg-purple-600 text-white px-4 py-2 rounded-lg hover:bg-purple-700 focus:outline-none focus:ring-2 focus:ring-purple-500 focus:ring-opacity-50 transition duration-300\"\r\n        >\r\n          Cerrar Sesión\r\n        </button>\r\n      </div>\r\n\r\n      <div class=\"grid grid-cols-1 md:grid-cols-2 gap-6\">\r\n        <!-- Subir Cartones -->\r\n        <div class=\"space-y-4\">\r\n          <h2 class=\"text-lg font-semibold text-gray-800\">Subir Cartones</h2>\r\n          <input\r\n            type=\"file\"\r\n            accept=\"image/*\"\r\n            multiple\r\n            @change=\"handleFileUpload\"\r\n            class=\"block w-full text-sm text-gray-500 file:mr-4 file:py-2 file:px-4 file:rounded-lg file:border-0 file:text-sm file:font-semibold file:bg-gray-100 file:text-gray-700 hover:file:bg-gray-200 transition duration-300\"\r\n          />\r\n          <button\r\n            @click=\"uploadCartons\"\r\n            class=\"w-full bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300\"\r\n          >\r\n            Subir Imágenes\r\n          </button>\r\n        </div>\r\n\r\n        <!-- Asignar Cartones -->\r\n        <div class=\"space-y-4\">\r\n          <h2 class=\"text-lg font-semibold text-gray-800\">Asignar Cartones por Lote</h2>\r\n          <div>\r\n            <label class=\"block text-sm font-medium text-gray-700\">Seleccionar Vendedor</label>\r\n            <select\r\n              v-model=\"selectedSeller\"\r\n              class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n            >\r\n              <option disabled value=\"\">Selecciona un vendedor</option>\r\n              <option v-for=\"seller in sellers\" :key=\"seller.id\" :value=\"seller.id\">{{ seller.nombre }}</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"grid grid-cols-2 gap-4\">\r\n            <div>\r\n              <label class=\"block text-sm font-medium text-gray-700\">Desde</label>\r\n              <select\r\n                v-model=\"cartonFrom\"\r\n                class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n              >\r\n                <option disabled value=\"\">Selecciona un cartón</option>\r\n                <option v-for=\"carton in availableCartons\" :key=\"carton.id\" :value=\"carton.id\">{{ carton.serial }}</option>\r\n              </select>\r\n            </div>\r\n            <div>\r\n              <label class=\"block text-sm font-medium text-gray-700\">Hasta</label>\r\n              <select\r\n                v-model=\"cartonTo\"\r\n                class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n              >\r\n                <option disabled value=\"\">Selecciona un cartón</option>\r\n                <option v-for=\"carton in availableCartons\" :key=\"carton.id\" :value=\"carton.id\">{{ carton.serial }}</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n          <button\r\n            @click=\"assignCartons\"\r\n            class=\"w-full bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300\"\r\n          >\r\n            Asignar Seleccionados\r\n          </button>\r\n          <p class=\"text-yellow-600 text-sm\">Por favor, selecciona un vendedor y un rango de cartones.</p>\r\n        </div>\r\n\r\n        <!-- Lista de Cartones -->\r\n        <div class=\"space-y-4\">\r\n          <h2 class=\"text-lg font-semibold text-gray-800\">Lista de Cartones</h2>\r\n          <div class=\"overflow-x-auto\">\r\n            <table class=\"min-w-full bg-white\">\r\n              <thead>\r\n                <tr>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Serial</th>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Estado</th>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Vendedor</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"carton in cartons\" :key=\"carton.id\">\r\n                  <td class=\"py-2 px-4 border-b\">{{ carton.serial }}</td>\r\n                  <td class=\"py-2 px-4 border-b\">{{ carton.estado }}</td>\r\n                  <td class=\"py-2 px-4 border-b\">{{ carton.vendedorId ? sellers.find(s => s.id === carton.vendedorId)?.nombre || 'Sin asignar' : 'Sin asignar' }}</td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <div class=\"flex space-x-4\">\r\n            <button\r\n              @click=\"refreshCartonList\"\r\n              class=\"bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300\"\r\n            >\r\n              Refrescar\r\n            </button>\r\n            <button\r\n              @click=\"downloadReport\"\r\n              class=\"bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300\"\r\n            >\r\n              Descargar Reporte\r\n            </button>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Lista de Vendedores -->\r\n        <div class=\"space-y-4\">\r\n          <h2 class=\"text-lg font-semibold text-gray-800\">Lista de Vendedores</h2>\r\n          <div class=\"overflow-x-auto\">\r\n            <table class=\"min-w-full bg-white\">\r\n              <thead>\r\n                <tr>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Nombre</th>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Saldo Generado</th>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Cartones Asignados</th>\r\n                  <th class=\"py-2 px-4 border-b text-left text-gray-600\">Acciones</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"seller in sellers\" :key=\"seller.id\">\r\n                  <td class=\"py-2 px-4 border-b\">{{ seller.nombre }}</td>\r\n                  <td class=\"py-2 px-4 border-b\">${{ seller.saldoGenerado }}</td>\r\n                  <td class=\"py-2 px-4 border-b\">{{ assignedCartons.filter(carton => carton.vendedorId === seller.id).length }}</td>\r\n                  <td class=\"py-2 px-4 border-b\">\r\n                    <button\r\n                      @click=\"viewSellerDetails(seller)\"\r\n                      class=\"bg-indigo-600 text-white px-4 py-1 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300\"\r\n                    >\r\n                      Ver Detalles\r\n                    </button>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n          <button\r\n            @click=\"showAddSellerModal = true\"\r\n            class=\"w-full bg-green-600 text-white px-4 py-2 rounded-lg hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-opacity-50 transition duration-300\"\r\n          >\r\n            Agregar Vendedor\r\n          </button>\r\n        </div>\r\n\r\n        <!-- Estadísticas -->\r\n        <div class=\"space-y-4 md:col-span-2\">\r\n          <h2 class=\"text-lg font-semibold text-gray-800\">Estadísticas</h2>\r\n          <div class=\"grid grid-cols-2 md:grid-cols-4 gap-4\">\r\n            <div class=\"text-center\">\r\n              <p class=\"text-gray-600\">Cartones Disponibles</p>\r\n              <p class=\"text-2xl font-bold text-gray-800\">{{ availableCartons.length }}</p>\r\n            </div>\r\n            <div class=\"text-center\">\r\n              <p class=\"text-gray-600\">Cartones Vendidos</p>\r\n              <p class=\"text-2xl font-bold text-gray-800\">{{ soldCartons.length }}</p>\r\n            </div>\r\n            <div class=\"text-center\">\r\n              <p class=\"text-gray-600\">Vendedores</p>\r\n              <p class=\"text-2xl font-bold text-gray-800\">{{ sellers.length }}</p>\r\n            </div>\r\n            <div class=\"text-center\">\r\n              <p class=\"text-gray-600\">Cartones Asignados</p>\r\n              <p class=\"text-2xl font-bold text-gray-800\">{{ assignedCartons.length }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Modal para Agregar Vendedor -->\r\n      <div v-if=\"showAddSellerModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50\">\r\n        <div class=\"bg-white p-8 rounded-xl shadow-2xl w-full max-w-md transform transition-all duration-500 opacity-0 animate-fade-in\">\r\n          <h3 class=\"text-xl font-semibold text-gray-800 mb-4\">Agregar Nuevo Vendedor</h3>\r\n          <form @submit.prevent=\"addSeller\" class=\"space-y-6\">\r\n            <div>\r\n              <label for=\"sellerEmail\" class=\"block text-sm font-medium text-gray-700\">Correo</label>\r\n              <input\r\n                v-model=\"newSellerEmail\"\r\n                id=\"sellerEmail\"\r\n                type=\"email\"\r\n                required\r\n                class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n                placeholder=\"vendedor1@example.com\"\r\n              />\r\n            </div>\r\n            <div>\r\n              <label for=\"sellerPassword\" class=\"block text-sm font-medium text-gray-700\">Contraseña</label>\r\n              <input\r\n                v-model=\"newSellerPassword\"\r\n                id=\"sellerPassword\"\r\n                type=\"password\"\r\n                required\r\n                class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n                placeholder=\"Ingrese la contraseña\"\r\n              />\r\n            </div>\r\n            <div>\r\n              <label for=\"sellerName\" class=\"block text-sm font-medium text-gray-700\">Nombre</label>\r\n              <input\r\n                v-model=\"newSellerName\"\r\n                id=\"sellerName\"\r\n                type=\"text\"\r\n                required\r\n                class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n                placeholder=\"Nombre del vendedor\"\r\n              />\r\n            </div>\r\n            <div class=\"flex justify-end space-x-4\">\r\n              <button\r\n                type=\"button\"\r\n                @click=\"showAddSellerModal = false\"\r\n                class=\"bg-gray-500 text-white px-4 py-2 rounded-lg hover:bg-gray-600 focus:outline-none focus:ring-2 focus:ring-gray-500 focus:ring-opacity-50 transition duration-300\"\r\n              >\r\n                Cancelar\r\n              </button>\r\n              <button\r\n                type=\"submit\"\r\n                class=\"bg-green-600 text-white px-4 py-2 rounded-lg hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-green-500 focus:ring-opacity-50 transition duration-300\"\r\n              >\r\n                Agregar\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Redirección para Vendedores -->\r\n    <div v-if=\"user && user.email.startsWith('vendedor')\" class=\"min-h-screen flex items-center justify-center\">\r\n      <p class=\"text-center text-white text-lg\">Redirigiendo...</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { auth } from '../firebase';\r\nimport { signInWithEmailAndPassword, onAuthStateChanged, signOut } from 'firebase/auth';\r\nimport { createUserWithEmailAndPassword } from 'firebase/auth';\r\nimport { db } from '../firebase';\r\nimport { doc, setDoc, collection, query, where, onSnapshot, getDocs, addDoc, updateDoc } from 'firebase/firestore';\r\nimport { getStorage, ref, uploadBytes, getDownloadURL } from 'firebase/storage';\r\n\r\nexport default {\r\n  name: 'LoginPage',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      error: '',\r\n      loading: false,\r\n      user: null,\r\n      showAddSellerModal: false,\r\n      newSellerEmail: '',\r\n      newSellerPassword: '',\r\n      newSellerName: '',\r\n      selectedSeller: '',\r\n      cartonFrom: '',\r\n      cartonTo: '',\r\n      sellers: [],\r\n      cartons: [],\r\n      availableCartons: [],\r\n      assignedCartons: [],\r\n      soldCartons: [],\r\n      files: []\r\n    };\r\n  },\r\n  mounted() {\r\n    onAuthStateChanged(auth, (user) => {\r\n      this.user = user;\r\n      if (user) {\r\n        if (user.email === 'master1@example.com') {\r\n          this.loadSellers();\r\n          this.loadCartons();\r\n          this.loadCartonStats();\r\n        } else if (user.email.startsWith('vendedor')) {\r\n          this.$router.push('/vendedor');\r\n        }\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    async login() {\r\n      this.loading = true;\r\n      this.error = '';\r\n      try {\r\n        console.log('Intentando iniciar sesión con:', this.email, this.password);\r\n        await signInWithEmailAndPassword(auth, this.email, this.password);\r\n        console.log('Sesión iniciada con éxito');\r\n      } catch (error) {\r\n        console.error('Error al iniciar sesión:', error.code, error.message);\r\n        this.error = `Error: ${error.code} - ${error.message}`;\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    async addSeller() {\r\n      if (!this.newSellerEmail || !this.newSellerPassword || !this.newSellerName) {\r\n        alert('Por favor, complete todos los campos.');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const userCredential = await createUserWithEmailAndPassword(auth, this.newSellerEmail, this.newSellerPassword);\r\n        const user = userCredential.user;\r\n        await setDoc(doc(db, 'vendedores', user.uid), {\r\n          email: this.newSellerEmail,\r\n          nombre: this.newSellerName,\r\n          saldoGenerado: 0\r\n        });\r\n        alert('Vendedor agregado exitosamente.');\r\n        this.showAddSellerModal = false;\r\n        this.newSellerEmail = '';\r\n        this.newSellerPassword = '';\r\n        this.newSellerName = '';\r\n        this.loadSellers();\r\n      } catch (error) {\r\n        console.error('Error al agregar vendedor:', error);\r\n        alert('Error al agregar vendedor: ' + error.message);\r\n      }\r\n    },\r\n    async logout() {\r\n      try {\r\n        await signOut(auth);\r\n        this.user = null;\r\n        this.$router.push('/login');\r\n      } catch (error) {\r\n        console.error('Error al cerrar sesión:', error);\r\n        alert('Error al cerrar sesión: ' + error.message);\r\n      }\r\n    },\r\n    loadSellers() {\r\n      const sellersQuery = query(collection(db, 'vendedores'));\r\n      onSnapshot(sellersQuery, (snapshot) => {\r\n        this.sellers = snapshot.docs.map(doc => ({\r\n          id: doc.id,\r\n          ...doc.data()\r\n        }));\r\n      });\r\n    },\r\n    loadCartons() {\r\n      const cartonsQuery = query(collection(db, 'cartones'));\r\n      onSnapshot(cartonsQuery, (snapshot) => {\r\n        this.cartons = snapshot.docs.map(doc => ({\r\n          id: doc.id,\r\n          ...doc.data()\r\n        }));\r\n      });\r\n    },\r\n    loadCartonStats() {\r\n      const availableQuery = query(\r\n        collection(db, 'cartones'),\r\n        where('vendedorId', '==', null),\r\n        where('estado', '==', 'disponible')\r\n      );\r\n      onSnapshot(availableQuery, (snapshot) => {\r\n        this.availableCartons = snapshot.docs.map(doc => ({\r\n          id: doc.id,\r\n          serial: doc.data().serial\r\n        })).sort((a, b) => parseInt(a.serial.replace('LM', '')) - parseInt(b.serial.replace('LM', '')));\r\n      });\r\n\r\n      const assignedQuery = query(\r\n        collection(db, 'cartones'),\r\n        where('estado', '==', 'asignado')\r\n      );\r\n      onSnapshot(assignedQuery, (snapshot) => {\r\n        this.assignedCartons = snapshot.docs.map(doc => ({\r\n          id: doc.id,\r\n          serial: doc.data().serial,\r\n          vendedorId: doc.data().vendedorId\r\n        }));\r\n      });\r\n\r\n      const soldQuery = query(\r\n        collection(db, 'cartones'),\r\n        where('estado', '==', 'vendido')\r\n      );\r\n      onSnapshot(soldQuery, (snapshot) => {\r\n        this.soldCartons = snapshot.docs.map(doc => ({\r\n          id: doc.id,\r\n          serial: doc.data().serial\r\n        }));\r\n      });\r\n    },\r\n    handleFileUpload(event) {\r\n      this.files = Array.from(event.target.files);\r\n    },\r\n    async uploadCartons() {\r\n      if (this.files.length === 0) {\r\n        alert('Por favor, selecciona al menos una imagen.');\r\n        return;\r\n      }\r\n\r\n      const storage = getStorage();\r\n      const uploadPromises = this.files.map(async (file, index) => {\r\n        const serial = `LM${Date.now()}${index}`;\r\n        const storageRef = ref(storage, `cartones/${serial}`);\r\n        await uploadBytes(storageRef, file);\r\n        const url = await getDownloadURL(storageRef);\r\n\r\n        await addDoc(collection(db, 'cartones'), {\r\n          serial: serial,\r\n          url: url,\r\n          estado: 'disponible',\r\n          vendedorId: null,\r\n          createdAt: new Date()\r\n        });\r\n      });\r\n\r\n      try {\r\n        await Promise.all(uploadPromises);\r\n        alert('Cartones subidos exitosamente.');\r\n        this.files = [];\r\n      } catch (error) {\r\n        console.error('Error al subir cartones:', error);\r\n        alert('Error al subir cartones: ' + error.message);\r\n     