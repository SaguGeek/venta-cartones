{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { auth } from '../firebase';\nimport { signInWithEmailAndPassword } from 'firebase/auth';\nexport default {\n  name: 'AdminLogin',\n  data() {\n    return {\n      email: '',\n      password: '',\n      error: '',\n      loading: false,\n      adminEmail: 'master1@example.com' // Correo fijo para administrador\n    };\n  },\n  methods: {\n    async login() {\n      this.loading = true;\n      this.error = '';\n      try {\n        if (this.email !== this.adminEmail) {\n          throw new Error('Solo el administrador master1@example.com puede iniciar sesión aquí.');\n        }\n        console.log('Intentando iniciar sesión con:', this.email, this.password);\n        const userCredential = await signInWithEmailAndPassword(auth, this.email, this.password);\n        console.log('Sesión iniciada con éxito:', userCredential.user);\n        this.$router.push('/');\n      } catch (error) {\n        console.error('Error al iniciar sesión:', error.code, error.message);\n        this.error = `Error: ${error.code} - ${error.message}`;\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["auth","signInWithEmailAndPassword","name","data","email","password","error","loading","adminEmail","methods","login","Error","console","log","userCredential","user","$router","push","code","message"],"sources":["D:\\Lotweb\\venta-cartones\\src\\components\\AdminLogin.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen flex items-center justify-center bg-gradient-to-br from-indigo-900 to-purple-300\">\r\n    <div class=\"bg-white p-8 rounded-xl shadow-2xl w-full max-w-md transform transition-all duration-500 opacity-0 animate-fade-in\">\r\n      <div class=\"text-center mb-6\">\r\n        <div class=\"flex justify-center mb-4\">\r\n          <span class=\"text-4xl font-bold text-indigo-600 animate-spin-slow\">VC</span>\r\n        </div>\r\n        <h2 class=\"text-3xl font-bold text-gray-800 mb-2\">Login Administrador</h2>\r\n        <p class=\"text-gray-600\">Inicia sesión como administrador</p>\r\n      </div>\r\n      <form @submit.prevent=\"login\" class=\"space-y-6\">\r\n        <div>\r\n          <label for=\"email\" class=\"block text-sm font-medium text-gray-700\">Correo Electrónico</label>\r\n          <input\r\n            v-model=\"email\"\r\n            type=\"email\"\r\n            id=\"email\"\r\n            class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n            placeholder=\"master1@example.com\"\r\n            required\r\n          />\r\n        </div>\r\n        <div>\r\n          <label for=\"password\" class=\"block text-sm font-medium text-gray-700\">Contraseña</label>\r\n          <input\r\n            v-model=\"password\"\r\n            type=\"password\"\r\n            id=\"password\"\r\n            class=\"mt-1 block w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-indigo-500 focus:border-indigo-500 transition duration-300\"\r\n            placeholder=\"Sagu7.98\"\r\n            required\r\n          />\r\n        </div>\r\n        <button\r\n          type=\"submit\"\r\n          :disabled=\"loading\"\r\n          class=\"w-full bg-indigo-600 text-white px-4 py-2 rounded-lg hover:bg-indigo-700 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-opacity-50 transition duration-300 flex items-center justify-center\"\r\n        >\r\n          <span v-if=\"loading\" class=\"flex items-center\">\r\n            <svg class=\"animate-spin h-5 w-5 mr-2 text-white\" viewBox=\"0 0 24 24\">\r\n              <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"></circle>\r\n              <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8V0C5.373 0 0 5.373 0 12h4zm2 5.291A7.962 7.962 0 014 12H0c0 3.042 1.135 5.824 3 7.938l3-2.647z\"></path>\r\n            </svg>\r\n            Cargando...\r\n          </span>\r\n          <span v-else>Iniciar Sesión</span>\r\n        </button>\r\n        <p v-if=\"error\" class=\"text-red-500 text-sm text-center\">{{ error }}</p>\r\n      </form>\r\n      <div class=\"mt-4 text-center\">\r\n        <p class=\"text-gray-600 text-sm\">¿Olvidaste tu contraseña? <a href=\"#\" class=\"text-indigo-600 hover:underline transition duration-300\">Recupérala</a></p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { auth } from '../firebase';\r\nimport { signInWithEmailAndPassword } from 'firebase/auth';\r\n\r\nexport default {\r\n  name: 'AdminLogin',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      error: '',\r\n      loading: false,\r\n      adminEmail: 'master1@example.com' // Correo fijo para administrador\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      this.loading = true;\r\n      this.error = '';\r\n      try {\r\n        if (this.email !== this.adminEmail) {\r\n          throw new Error('Solo el administrador master1@example.com puede iniciar sesión aquí.');\r\n        }\r\n        console.log('Intentando iniciar sesión con:', this.email, this.password);\r\n        const userCredential = await signInWithEmailAndPassword(auth, this.email, this.password);\r\n        console.log('Sesión iniciada con éxito:', userCredential.user);\r\n        this.$router.push('/');\r\n      } catch (error) {\r\n        console.error('Error al iniciar sesión:', error.code, error.message);\r\n        this.error = `Error: ${error.code} - ${error.message}`;\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Animación de fade-in para la tarjeta */\r\n.animate-fade-in {\r\n  animation: fadeIn 0.5s ease-in-out forwards;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n/* Animación de rotación lenta para el logo */\r\n.animate-spin-slow {\r\n  animation: spin 2s linear infinite;\r\n}\r\n\r\n@keyframes spin {\r\n  from {\r\n    transform: rotate(0deg);\r\n  }\r\n  to {\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n</style>"],"mappings":";AAyDA,SAASA,IAAG,QAAS,aAAa;AAClC,SAASC,0BAAyB,QAAS,eAAe;AAE1D,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,KAAK;MACdC,UAAU,EAAE,qBAAoB,CAAE;IACpC,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI,CAACH,OAAM,GAAI,IAAI;MACnB,IAAI,CAACD,KAAI,GAAI,EAAE;MACf,IAAI;QACF,IAAI,IAAI,CAACF,KAAI,KAAM,IAAI,CAACI,UAAU,EAAE;UAClC,MAAM,IAAIG,KAAK,CAAC,sEAAsE,CAAC;QACzF;QACAC,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAE,IAAI,CAACT,KAAK,EAAE,IAAI,CAACC,QAAQ,CAAC;QACxE,MAAMS,cAAa,GAAI,MAAMb,0BAA0B,CAACD,IAAI,EAAE,IAAI,CAACI,KAAK,EAAE,IAAI,CAACC,QAAQ,CAAC;QACxFO,OAAO,CAACC,GAAG,CAAC,4BAA4B,EAAEC,cAAc,CAACC,IAAI,CAAC;QAC9D,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,EAAE,OAAOX,KAAK,EAAE;QACdM,OAAO,CAACN,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAACY,IAAI,EAAEZ,KAAK,CAACa,OAAO,CAAC;QACpE,IAAI,CAACb,KAAI,GAAI,UAAUA,KAAK,CAACY,IAAI,MAAMZ,KAAK,CAACa,OAAO,EAAE;MACxD,UAAU;QACR,IAAI,CAACZ,OAAM,GAAI,KAAK;MACtB;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}